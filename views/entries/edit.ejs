<%- include('../partials/html-head') %>
<%- include('../partials/nav') %>

<section>
  <h1>Edit your entry </h1>
  <form 
    action="/entry/<%= entry._id %>?_method=PUT"
    method="POST"
  >
    <label>
      Name: 
      <input 
        type="text"
        name="name" 
        value="<%= entry.name %>" 
        autocomplete="off"
      >
    </label>
  
    <button type="submit">Edit your entry</button>
  </form>
</section>

